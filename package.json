{
  "name": "@versui/cli",
  "version": "1.5.4",
  "description": "Deploy static sites to Walrus decentralized storage",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "versui": "./src/index.js"
  },
  "scripts": {
    "dev": "node src/index.js",
    "lint": "eslint . && prettier . --check && npm run typecheck",
    "typecheck": "tsc --build",
    "format": "prettier . --write && eslint . --fix",
    "test": "NODE_NO_WARNINGS=1 node --test test/**/*.test.js",
    "test:coverage": "c8 --reporter=text --reporter=html --reporter=lcov npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Versui/versui-cli.git"
  },
  "keywords": [
    "walrus",
    "sui",
    "blockchain",
    "web3",
    "deployment",
    "cli"
  ],
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Versui/versui-cli/issues"
  },
  "homepage": "https://github.com/Versui/versui-cli#readme",
  "dependencies": {
    "@mysten/sui": "^1.45.0",
    "@mysten/suins": "^0.9.10",
    "@mysten/walrus": "^0.8.4",
    "boxen": "^8.0.1",
    "chalk": "^5.6.2",
    "cli-table3": "^0.6.5",
    "commander": "^14.0.2",
    "figlet": "^1.9.4",
    "gradient-string": "^3.0.0",
    "log-update": "^7.0.2",
    "mime": "^4.1.0",
    "minimatch": "^10.1.1",
    "ora": "^9.0.0",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "@types/prompts": "^2.4.9",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "c8": "^10.1.3",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "^16.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "prettier": "^3.6.2",
    "typescript": "^5.7.3"
  }
}
